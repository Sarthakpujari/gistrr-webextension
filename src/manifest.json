{
  "name": "Bookmark Override",
  "description": "Simple bookmark manager",
  "permissions": ["bookmarks", "tabs", "activeTab"],
  "action": {
    "default_popup": "pages/popup/index.html"
  },
  "chrome_url_overrides": {
    "bookmarks": "pages/bookmarks/index.html"
  },
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
